<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="homeviewmodel"
            type="com.alatheer.noamany.HomeViewModel" />
    </data>
    <RelativeLayout
        android:id="@+id/relative_content"
        tools:context="com.alatheer.unitedgym.Activities.HomeActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F6F6F6">
        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_50sdp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true" />
        <FrameLayout
            android:id="@+id/frame_navigation"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/home_bottomnavigation"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:outlineSpotShadowColor="@color/lightblack"
                android:outlineAmbientShadowColor="@color/lightblack"
                app:menu="@menu/menu_navigation"
                app:labelVisibilityMode="unlabeled"
                app:itemTextColor="@drawable/bottom_nav_color"
                app:itemIconTint="@drawable/bottom_nav_color"
                app:itemIconSize="@dimen/_24sdp"
                android:background="@color/white"
                app:itemBackground="@drawable/selector_checked_uncheked"/>
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/home_bottomnavigation2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:outlineSpotShadowColor="@color/white"
                android:outlineAmbientShadowColor="@color/white"
                app:menu="@menu/menu_navigation2"
                app:labelVisibilityMode="labeled"
                android:elevation="@dimen/_5sdp"
                app:itemTextColor="@drawable/bottom_nav_color"
                app:itemIconTint="@drawable/bottom_nav_color"
                android:visibility="gone"
                app:itemIconSize="@dimen/_24sdp"
                android:background="@color/lightblack"/>
        </FrameLayout>

    </RelativeLayout>
</layout>

